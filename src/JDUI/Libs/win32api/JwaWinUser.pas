  {$EXTERNALSYM PDLGITEMTEMPLATE}

  LPDLGITEMTEMPLATEA = ^DLGITEMTEMPLATE;
  {$EXTERNALSYM LPDLGITEMTEMPLATE}
  LPDLGITEMTEMPLATEW = ^DLGITEMTEMPLATE;
  {$EXTERNALSYM LPDLGITEMTEMPLATE}

{$IFDEF UNICODE}
  PDLGITEMTEMPLATE = PDLGITEMTEMPLATEW;
  {$EXTERNALSYM PDLGITEMTEMPLATE}
  LPDLGITEMTEMPLATE = PDLGITEMTEMPLATEW;
  {$EXTERNALSYM LPDLGITEMTEMPLATE}
